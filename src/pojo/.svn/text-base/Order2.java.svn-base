package pojo;

import java.util.List;

public class Order2{
	/**
	 * 总订单
	 */
	private Integer id;
	private Integer teamid; //用户ID
	private Integer courseid;//商品ID
	private String  oid;//订单号
	private String  coursetitle;//商品的标题
	private Integer type;//类型0业务；1商标
	
	private String  courseimg;//商品的显示图片
	private Double  price;//商品的价格
	private Double  allprice;//订单总价格
	private Integer status=0;//0未付款，1已付款，2-退款订单
	private Integer tuikuanstatus;//退款状态0退款中;;1-已完成;2-拒绝；3-等待审核
	
	private String time=TimeUtil.getString();//付款时间
	private String time1=TimeUtil.getString();//下单时间
	
	private String team; //用户名
	private Integer num=1;//数量
	
	private Integer isfanyong=0;//0能反佣；1不能反佣
	
	public Integer getIsfanyong() {
		return isfanyong;
	}
	public void setIsfanyong(Integer isfanyong) {
		this.isfanyong = isfanyong;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getTeamid() {
		return teamid;
	}
	public void setTeamid(Integer teamid) {
		this.teamid = teamid;
	}
	public Integer getCourseid() {
		return courseid;
	}
	public void setCourseid(Integer courseid) {
		this.courseid = courseid;
	}
	public String getOid() {
		return oid;
	}
	public void setOid(String oid) {
		this.oid = oid;
	}
	public String getCoursetitle() {
		return coursetitle;
	}
	public void setCoursetitle(String coursetitle) {
		this.coursetitle = coursetitle;
	}
	public Integer getType() {
		return type;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	public String getCourseimg() {
		return courseimg;
	}
	public void setCourseimg(String courseimg) {
		this.courseimg = courseimg;
	}
	public Double getPrice() {
		return price;
	}
	public void setPrice(Double price) {
		this.price = price;
	}
	public Integer getStatus() {
		return status;
	}
	public void setStatus(Integer status) {
		this.status = status;
	}
	public String getTime() {
		return time;
	}
	public void setTime(String time) {
		this.time = time;
	}
	public String getTime1() {
		return time1;
	}
	public void setTime1(String time1) {
		this.time1 = time1;
	}
	public String getTeam() {
		return team;
	}
	public void setTeam(String team) {
		this.team = team;
	}
	public Integer getTuikuanstatus() {
		return tuikuanstatus;
	}
	public void setTuikuanstatus(Integer tuikuanstatus) {
		this.tuikuanstatus = tuikuanstatus;
	}
	public Integer getNum() {
		return num;
	}
	public Double getAllprice() {
		return allprice;
	}
	public void setAllprice(Double allprice) {
		this.allprice = allprice;
	}
	public void setNum(Integer num) {
		this.num = num;
	}
	@Override
	public String toString() {
		return "Order2 [allprice=" + allprice + ", courseid=" + courseid
				+ ", courseimg=" + courseimg + ", coursetitle=" + coursetitle
				+ ", id=" + id + ", num=" + num + ", oid=" + oid + ", price="
				+ price + ", status=" + status + ", team=" + team + ", teamid="
				+ teamid + ", time=" + time + ", time1=" + time1
				+ ", tuikuanstatus=" + tuikuanstatus + ", type=" + type
				+ ", getAllprice()=" + getAllprice() + ", getCourseid()="
				+ getCourseid() + ", getCourseimg()=" + getCourseimg()
				+ ", getCoursetitle()=" + getCoursetitle() + ", getId()="
				+ getId() + ", getNum()=" + getNum() + ", getOid()=" + getOid()
				+ ", getPrice()=" + getPrice() + ", getStatus()=" + getStatus()
				+ ", getTeam()=" + getTeam() + ", getTeamid()=" + getTeamid()
				+ ", getTime()=" + getTime() + ", getTime1()=" + getTime1()
				+ ", getTuikuanstatus()=" + getTuikuanstatus() + ", getType()="
				+ getType() + ", getClass()=" + getClass() + ", hashCode()="
				+ hashCode() + ", toString()=" + super.toString() + "]";
	}
	
	
}
