package pojo;

import java.util.List;

public class Talk {
	
	private Integer id;
	private Integer forumId;
	private Integer teamId;
	private String content;
	private String time=TimeUtil.getString();
	private String showTime=TimeUtil.getStringShort();
	//临时字段
	private Team team;
	private List<TalPhoto> talPhotos;
	private List<BackTalk> backTalks;
	
	
	public List<BackTalk> getBackTalks() {
		return backTalks;
	}
	public void setBackTalks(List<BackTalk> backTalks) {
		this.backTalks = backTalks;
	}
	public List<TalPhoto> getTalPhotos() {
		return talPhotos;
	}
	public void setTalPhotos(List<TalPhoto> talPhotos) {
		this.talPhotos = talPhotos;
	}
	public Team getTeam() {
		return team;
	}
	public void setTeam(Team team) {
		this.team = team;
	}
	public String getShowTime() {
		return showTime;
	}
	public void setShowTime(String showTime) {
		this.showTime = showTime;
	}
	public Integer getTeamId() {
		return teamId;
	}
	public void setTeamId(Integer teamId) {
		this.teamId = teamId;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getForumId() {
		return forumId;
	}
	public void setForumId(Integer forumId) {
		this.forumId = forumId;
	}
	public String getTime() {
		return time;
	}
	public void setTime(String time) {
		this.time = time;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	
	

}
