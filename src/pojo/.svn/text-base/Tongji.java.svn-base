package pojo;
/*
 * 添加财务管理
 */
public class Tongji {
	private Integer id;
	private String time = TimeUtil.getStringShort();//财务统计时间
	private Integer huiyuannum;//新增会员人数
	private Integer huiyuanprice;//会员收入金额
	
	private Integer coursenum;//课程数量
	private Integer courseprice;//课程收入
	
	private Integer productnum;//积分商城售出订单数
	private Integer productprice;//积分商城收入金额
	private Integer productpoint;//积分商城收入积分
	
	private Integer price;//总收入金额
	
	
	
	public Integer getId() {
		return id;
	}

	public String getTime() {
		return time;
	}

	public Integer getCoursenum() {
		return coursenum;
	}

	public Integer getCourseprice() {
		return courseprice;
	}

	public Integer getProductnum() {
		return productnum;
	}

	public Integer getProductprice() {
		return productprice;
	}

	public Integer getProductpoint() {
		return productpoint;
	}

	public Integer getHuiyuannum() {
		return huiyuannum;
	}

	public Integer getHuiyuanprice() {
		return huiyuanprice;
	}

	public Integer getPrice() {
		return price;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public void setTime(String time) {
		this.time = time;
	}

	public void setCoursenum(Integer coursenum) {
		this.coursenum = coursenum;
	}

	public void setCourseprice(Integer courseprice) {
		this.courseprice = courseprice;
	}

	public void setProductnum(Integer productnum) {
		this.productnum = productnum;
	}

	public void setProductprice(Integer productprice) {
		this.productprice = productprice;
	}

	public void setProductpoint(Integer productpoint) {
		this.productpoint = productpoint;
	}

	public void setHuiyuannum(Integer huiyuannum) {
		this.huiyuannum = huiyuannum;
	}

	public void setHuiyuanprice(Integer huiyuanprice) {
		this.huiyuanprice = huiyuanprice;
	}

	public void setPrice(Integer price) {
		this.price = price;
	}
	
	
	
}
