package pojo;

/**
 *  购买课程的订单 
 */

public class Order1 {
	
	private Integer id;
	private Integer teamid; //用户ID
	private Integer courseid;//课程ID
	private String  oid;//订单号
	private String  coursetitle;//课程的标题
	
	private String  courseimg;//课程的显示图片
	private Double  price;//课程的价格
	private Integer status=0;//0未付款，1已付款
	
	private String time=TimeUtil.getString();//付款时间
	
	
	private String team; //用户名
	
	
	//暂时不用2017-12-29
	private Integer contentid=0;//课时Id(记录上次观看的位置)
	private Integer pingfen;//课程打分
	private Integer teacherid=0;//课程教师的ID
	
	private String course;//课程名
	private String phone;//手机号码
	
	
	public Integer getTeacherid() {
		return teacherid;
	}
	public void setTeacherid(Integer teacherid) {
		this.teacherid = teacherid;
	}
	public Integer getPingfen() {
		return pingfen;
	}
	public void setPingfen(Integer pingfen) {
		this.pingfen = pingfen;
	}
	public String getPhone() {
		return phone;
	}
	public void setPhone(String phone) {
		this.phone = phone;
	}
	public String getTeam() {
		return team;
	}
	public String getCourse() {
		return course;
	}
	public void setTeam(String team) {
		this.team = team;
	}
	public void setCourse(String course) {
		this.course = course;
	}
	public Integer getStatus() {
		return status;
	}
	public void setStatus(Integer status) {
		this.status = status;
	}
	public Double getPrice() {
		return price;
	}
	public void setPrice(Double price) {
		this.price = price;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getTeamid() {
		return teamid;
	}
	public void setTeamid(Integer teamid) {
		this.teamid = teamid;
	}
	public Integer getCourseid() {
		return courseid;
	}
	public void setCourseid(Integer courseid) {
		this.courseid = courseid;
	}
	public String getOid() {
		return oid;
	}
	public void setOid(String oid) {
		this.oid = oid;
	}
	public String getTime() {
		return time;
	}
	public void setTime(String time) {
		this.time = time;
	}
	public String getCoursetitle() {
		return coursetitle;
	}
	public void setCoursetitle(String coursetitle) {
		this.coursetitle = coursetitle;
	}
	
	public String getCourseimg() {
		return courseimg;
	}
	public void setCourseimg(String courseimg) {
		this.courseimg = courseimg;
	}
	public Integer getContentid() {
		return contentid;
	}
	public void setContentid(Integer contentid) {
		this.contentid = contentid;
	}
}