package pojo;

/**
 *消费记录
 */
public class PayRecord {

	private Integer id;
	private Integer teamid;//用户id
	private Integer style=2; //0-积分 2-佣金
	private String time = TimeUtil.getString();//消费时间
	

	
	
	//关于佣金消费的
	private String yjcontent;//佣金消费事件
	private Double yjpay;//佣金消费数量
	
	private Integer status=0;//提现状态 0申请，1同意，2拒绝
	
	private String name;//开户姓名
	private String kahao;//银行卡号
	private String yinghang;//开户银行上一个下项目
	
	
	
	//暂时不用
	private Integer num;//购买数量
	//关于积分消费的
	private String jfcontent;//积分消费事件
	private Integer pay;//积分消费数量
	
	
	
	public Integer getStatus() {
		return status;
	}
	public String getName() {
		return name;
	}
	public String getKahao() {
		return kahao;
	}
	public String getYinghang() {
		return yinghang;
	}
	public void setStatus(Integer status) {
		this.status = status;
	}
	public void setName(String name) {
		this.name = name;
	}
	public void setKahao(String kahao) {
		this.kahao = kahao;
	}
	public void setYinghang(String yinghang) {
		this.yinghang = yinghang;
	}
	public Double getYjpay() {
		return yjpay;
	}
	public void setYjpay(Double yjpay) {
		this.yjpay = yjpay;
	}




	public String getJfcontent() {
		return jfcontent;
	}
	public void setJfcontent(String jfcontent) {
		this.jfcontent = jfcontent;
	}
	public String getYjcontent() {
		return yjcontent;
	}
	public void setYjcontent(String yjcontent) {
		this.yjcontent = yjcontent;
	}
	public Integer getNum() {
		return num;
	}
	public void setNum(Integer num) {
		this.num = num;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getStyle() {
		return style;
	}
	public void setStyle(Integer style) {
		this.style = style;
	}
	public String getTime() {
		return time;
	}
	public void setTime(String time) {
		this.time = time;
	}
	public Integer getTeamid() {
		return teamid;
	}
	public void setTeamid(Integer teamid) {
		this.teamid = teamid;
	}
	public Integer getPay() {
		return pay;
	}
	public void setPay(Integer pay) {
		this.pay = pay;
	}
	
	
	
}
