package pojo;

/**
 *   课程
 */
public class Course {

	private Integer id;//id(主键)
	private String title;//标题
	private String introduce;//课程详情
	
	private String teacher;//讲师
	private Integer teacherId;//讲师ID
	
	private Integer classify2id;//分类ID
	
	private String time=TimeUtil.getStringShort();//上传时间
	private String img;//当前显示封面
	
	private Integer status;//状态(0-普通，1-活动，2-已下架)
	private Double money=0.0;//课程价格
	private Double twomoney=0.0;//原价
	private Integer payNum=0;//购买人数(报名)
	private Integer readNum=0;//观看人数
	private Integer collectnum=0;//收藏人数
	private Integer period=0;//课时
	
	private String bofang;//临时字段，这次播放的视频地址
	private String details;//简介
	
	
	//临时字段
	private Integer collectid;//收藏的id
	private Integer order1id;//订单的id
	
	
	public Integer getCollectnum() {
		return collectnum;
	}
	public void setCollectnum(Integer collectnum) {
		this.collectnum = collectnum;
	}
	public Integer getOrder1id() {
		return order1id;
	}
	public void setOrder1id(Integer order1id) {
		this.order1id = order1id;
	}
	public Double getTwomoney() {
		return twomoney;
	}
	public void setTwomoney(Double twomoney) {
		this.twomoney = twomoney;
	}
	public Integer getClassify2id() {
		return classify2id;
	}
	public void setClassify2id(Integer classify2id) {
		this.classify2id = classify2id;
	}
	public String getBofang() {
		return bofang;
	}
	public void setBofang(String bofang) {
		this.bofang = bofang;
	}
	public Integer getTeacherId() {
		return teacherId;
	}
	public void setTeacherId(Integer teacherId) {
		this.teacherId = teacherId;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	
	public Integer getPayNum() {
		return payNum;
	}
	public void setPayNum(Integer payNum) {
		this.payNum = payNum;
	}
	public Integer getReadNum() {
		return readNum;
	}
	public void setReadNum(Integer readNum) {
		this.readNum = readNum;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public Integer getStatus() {
		return status;
	}
	public void setStatus(Integer status) {
		this.status = status;
	}
	public String getDetails() {
		return details;
	}
	public void setDetails(String details) {
		this.details = details;
	}
	public String getTeacher() {
		return teacher;
	}
	public void setTeacher(String teacher) {
		this.teacher = teacher;
	}
	public String getTime() {
		return time;
	}
	public void setTime(String time) {
		this.time = time;
	}
	public String getImg() {
		return img;
	}
	public void setImg(String img) {
		this.img = img;
	}
	
	public Integer getPeriod() {
		return period;
	}
	public void setPeriod(Integer period) {
		this.period = period;
	}


	public String getIntroduce() {
		return introduce;
	}
	public Integer getCollectid() {
		return collectid;
	}
	public void setCollectid(Integer collectid) {
		this.collectid = collectid;
	}
	public void setIntroduce(String introduce) {
		this.introduce = introduce;
	}
	public Double getMoney() {
		return money;
	}
	public void setMoney(Double money) {
		this.money = money;
	}


	
	

	
}
