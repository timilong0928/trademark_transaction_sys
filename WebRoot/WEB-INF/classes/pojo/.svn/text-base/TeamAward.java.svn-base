package pojo;

/**
 *  用户的奖励
 */
public class TeamAward {

	private Integer id;
	private Integer type=4;//积分来源 0-签到 1-连续签到 2-购买课程3-评论 ;4-表示佣金
	
	private Integer teamId=0;//奖励的用户id
	
	private String time=TimeUtil.getString();//奖励或者花费时间
	
	
	
	
	//返佣的记录字段，要显示返佣人，数量，返佣事件
	private String fanyongname;//返佣人的昵称
	private Double yongjinaward=0.0;//返佣的数量
	private Integer teamId1=0;//返佣来源的用户id
	
	
	
	
	
	//暂时不用
	private Integer courseId;//购买(分享)的课程
	private Integer postId; //分享资讯//暂时不用
	private String yjcontent;//奖励事件
	//积分
	private Integer point=0;//奖励积分
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getType() {
		return type;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	public Integer getPoint() {
		return point;
	}
	public void setPoint(Integer point) {
		this.point = point;
	}
	public String getTime() {
		return time;
	}
	public void setTime(String time) {
		this.time = time;
	}
	public Integer getTeamId() {
		return teamId;
	}
	public void setTeamId(Integer teamId) {
		this.teamId = teamId;
	}
	public String getFanyongname() {
		return fanyongname;
	}
	public void setFanyongname(String fanyongname) {
		this.fanyongname = fanyongname;
	}
	public Double getYongjinaward() {
		return yongjinaward;
	}
	public void setYongjinaward(Double yongjinaward) {
		this.yongjinaward = yongjinaward;
	}
	
	public String getYjcontent() {
		return yjcontent;
	}
	public void setYjcontent(String yjcontent) {
		this.yjcontent = yjcontent;
	}
	public Integer getCourseId() {
		return courseId;
	}
	public void setCourseId(Integer courseId) {
		this.courseId = courseId;
	}
	public Integer getPostId() {
		return postId;
	}
	public void setPostId(Integer postId) {
		this.postId = postId;
	}
	public Integer getTeamId1() {
		return teamId1;
	}
	public void setTeamId1(Integer teamId1) {
		this.teamId1 = teamId1;
	}
	


	

	
	
}
