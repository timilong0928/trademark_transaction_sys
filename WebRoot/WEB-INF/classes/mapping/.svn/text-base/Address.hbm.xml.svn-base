<?xml version="1.0" encoding="utf-8"?> 

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 

"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="pojo">

	<class name="Address" table="Address" catalog="shangbiao18">

		<id name="id" type="integer">
			<column name="id" />
			<generator class="native"></generator>
		</id>
		
		<property name="province" type="string">
			<column name="province" length="100" />
		</property>
		
		<property name="city" type="string">
			<column name="city" length="300" />
		</property>
		
		<property name="qu" type="string">
			<column name="qu" length="100" />
		</property>

		<property name="address" type="string">
			<column name="address" length="300" />
		</property>

		<property name="userId" type="integer">
			<column name="userId" length="11"/>
		</property>
		
		<property name="isMoren" type="integer">
			<column name="isMoren" length="11"/>
		</property>

		<property name="name" type="string">
			<column name="name" length="300" />
		</property>
		
		<property name="tel" type="string">
			<column name="tel" length="300" />
		</property>
		

	</class>
</hibernate-mapping>