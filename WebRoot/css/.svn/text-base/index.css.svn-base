.mainbody>div{
	border-top:4px solid #E9E9E9;
	padding: 0 5px;
}
#lunbo{
	padding:0;
	border-top:4px #E9E9E9;
}
.search{
	padding:0;
	border-top:4px #E9E9E9;
}

.partzero img{
	width: 100%;
}
.nav{
	padding: 10px;
	padding-left: 12px;
}
.nav>div{
	display: inline-block;
	text-align: center;
	width: 19%;
	margin-bottom: 5px;
	font-size: 12px;
	padding: 8px;
}
.nav img{
	width: 100%;
	border-radius:50%; 
}

.coupon{
	padding: 0 !important;
}

.cwrap{
	background-color:#3DA663;
	padding: 5px 0;
}

.cwrap>div{
	display:inline-block;
	width: 50%;
	padding: 0 15px;
}
.cwrap>div:first-child{
	border-right: 1px solid white;
}
.cwrap>div img{
	width: 100%;
}

/***product**/
.cut{
	padding-top: 10px;
}

.cut .cup{
	padding-top:5px;
	border-bottom:1px solid gray;
	text-align: center;
	font-size: 14px;
	height: 40px;
	position: relative;
}

.cup>span,.cup>img{
	vertical-align: middle;
	height: 25px;
	margin-left: 3px;
}
.cup .more{
	display: inline-block;
	position: absolute;
	right:5px;
	top:8px;
}

.plistup{
	padding-top:5px;
	margin-left:5px;
	text-align: left;
	font-size: 14px;
	height: 40px;
	position: relative;
	border-top:1px solid gray;
}

.plistup>span,.plistup>img{
	vertical-align: middle;
	height: 25px;
	margin-left: 3px;
}
.plistup .more{
	display: inline-block;
	position: absolute;
	right:5px;
	top:8px;
}

.productlist{
	vertical-align: top;
	margin-left: -5px;
}
.product{
	margin-top:10px;
	width: 46%;
	display: inline-block;
	vertical-align: top;
	margin-left: 10px;
}
.product img{
	width: 100%;
}
.product p{
	line-height:16px;
	margin: 0;
	padding:0;
}
.imgwrap{
	width: 100%;
	overflow: hidden;
}
.pdown p{
	margin-top: 5px;
}

.pdown p:nth-child(2){
	color: #989898;
	text-decoration: line-through;
}
.pdown p:nth-child(3){
	color: #FF752C;
	font-size: 18px;
}
.pdown p:nth-child(3) span.tuan{
	color: #989898;
	font-size: 14px;
}
.pdown button{
	background-color: #FA9F1D;
	color: white;
	padding: 4px 8px;
	float: right;
	margin-top: -10px;
}

/**search**/

.search{
	padding: 5px 5px;
}

.search .sup{
	width: 100%;
	text-align: left;
	position: relative;
}
.sup img{
	height: 50px;
}

.sup input{
	margin:10px auto;
	margin-bottom:5px;
	margin-left:5px;
	width: 67%;
	border-radius: 5px;
	border: 2px solid #EEEEEE;
	height: 35px;
	padding-right:25px;
	background: url(../img/search.png) no-repeat top 5px right 0/25px auto;
	background-color:#F6F6F6;
}
.sup a{
    display: block;
    width: 27px;
    height: 27px;
    position: absolute;
    right: 45px;
    top: 14px;
}
.sup span{
	background:url(../img/shaixuan.png) no-repeat top 0 left 0/26px 26px;
	padding:5px 15px;
	margin-left: 5px;
	
}
.sdown{
	display:none;
	padding: 0 20px;
	text-align: center;
	font-size: 16px;
}
.sdown input{
	width: 40%;
	border: 1px solid #3DA663;
	border-radius:5px;
	height: 30px;
}

.sdown button{
	margin-top:10px;
	border-radius:5px;
	background-color: #3DA663;
	color: white;
	border:1px solid ;
	padding: 5px 20px;
	margin-bottom: 10px;

}